  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch bot-worker",
      "request": "launch",
      "runtimeArgs": [
        "run",
        "dev:bot-worker"
      ],
      "env": {
        "SERVICE_NAME": "bot-worker",
        "RABBITMQ_QUEUE": "bot_worker_queue",
        "RABBITMQ_PREFETCH": "1",
      },
      "runtimeExecutable": "npm",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "node",
      "console": "integratedTerminal",
    }
  ]
}